<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/20.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bd.edu.seu.studentmanagement.popupController.DeleteStudentController">
   <children>
      <Label layoutX="131.0" layoutY="130.0" text="Student ID:">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <TextField fx:id="deleteStudentField" layoutX="62.0" layoutY="185.0" prefHeight="24.0" prefWidth="276.0">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <Button layoutX="158.0" layoutY="258.0" mnemonicParsing="false" onAction="#deleteStudentButton" text="Delete">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Label layoutX="91.0" layoutY="49.0" text="Delete Student">
         <font>
            <Font name="System Bold" size="31.0" />
         </font>
      </Label>
      <Label fx:id="errorMessage" layoutX="53.0" layoutY="220.0" text="There is no student with this ID" textFill="#ff0101">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
